{
  "build": {
    "beforeDevCommand": "mkdir -p /app/dist && echo 'Directory created'",
    "beforeBuildCommand": "npm run build:frontend",
    "devPath": "/app/dist",
    "distDir": "/app/dist"
  },
  "package": {
    "productName": "tauri-youtube-oauth",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "identifier": "com.ytlite.oauth",
      "icon": []
    },
    "allowlist": {
      "all": true
    }
  }
}
